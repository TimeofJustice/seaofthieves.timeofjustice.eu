svgIcon : { size : Int, css : List Css.Style, path : String } -> Html msg
svgIcon settings =
    svg
        [ viewBox "0 0 24 24"
        , width (String.fromInt settings.size)
        , height (String.fromInt settings.size)
        , css settings.css
        ]
        [ path
            [ d settings.path
            ]
            []
        ]


